{
  "name": "eu.boulay.blog.docpad",
  "version": "0.0.1",
  "description": "Florian's website based on Pure CSS library",
  "author": "Florian Boulay <florian.boulay@gmail.com> (http://blog.boulay.eu)",
  "license": "MIT",
  "engines": {
    "node": "4",
    "npm": "2"
  },
  "maintainers": [
    {
      "name": "Florian Boulay",
      "email": "florian.boulay@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fboulay/website.git"
  },
  "dependencies": {
    "docpad": "~6.78.6",
    "docpad-plugin-asciidoc": "~2.0.0",
    "docpad-plugin-cleancss": "~2.1.0",
    "docpad-plugin-cleanurls": "~2.8.1",
    "docpad-plugin-datefromfilename": "~2.0.4",
    "docpad-plugin-eco": "~2.1.0",
    "docpad-plugin-highlightjs": "~2.4.0",
    "docpad-plugin-htmlmin": "~2.4.0",
    "docpad-plugin-moment": "~2.0.2",
    "docpad-plugin-paged": "~2.4.0",
    "docpad-plugin-rss": "~2.3.1",
    "docpad-plugin-services": "~2.6.0"
  },
  "devDependencies": {
    "docpad-plugin-livereload": "~2.7.1"
  },
  "keywords": [
    "docpad",
    "blog",
    "docker"
  ],
  "main": "node_modules/.bin/docpad-server",
  "scripts": {
    "start": "node_modules/.bin/docpad-server",
    "test": "node_modules/.bin/docpad generate --debug --silent",
